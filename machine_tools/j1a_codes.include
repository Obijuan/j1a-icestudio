// J1a translation file ver. 0.1
// format: mnemonic=<hexcode0>,<hexcode1>...<hexcodeN>

// no operation
nop=0x6000
// add
add=0x6202
// xor
xor=0x6502
// and
and=0x6302
// or
or=0x6402
// not
invert=0x6600
// equal
eq=0x6702
// less than
lt=0x6802
