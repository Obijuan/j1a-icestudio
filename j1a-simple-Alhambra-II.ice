        {
          "id": "e69a332a-f12a-4bcc-a832-c6e223c65ecd",
          "type": "basic.code",
          "data": {
            "code": "reg insn;\n//-- Definicin de la tabla\n//-- Tabla de TAM elementos de M bits\nreg [15:0] tabla[0:4095];\n\n//-- Read the table\nalways @(posedge clk)\n  insn <= tabla[code_addr[11:0]];\n\n//-- Puerto escritura\nalways @(posedge clk)\n  if (mem_wr)\n      tabla[mem_addr[12:1]] <= d;\n\n//-- Init table from PROG parameter\ninitial begin\n  if (PROG) $readmemh(PROG, tabla);\nend\n",
            "params": [
              {
                "name": "PROG"
              }
            ],
            "ports": {
              "in": [
                {
                  "name": "clk"
                },
                {
                  "name": "mem_addr",
                  "range": "[15:0]",
                  "size": 16
                },
                {
                  "name": "code_addr",
                  "range": "[12:0]",
                  "size": 13
                },
                {
                  "name": "mem_wr"
                },
                {
                  "name": "d",
                  "range": "[15:0]",
                  "size": 16
                }
              ],
              "out": [
                {
                  "name": "insn",
                  "range": "[15:0]",
                  "size": 16
                }
              ]
            }
          },
          "position": {
